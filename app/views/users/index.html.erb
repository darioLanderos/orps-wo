 <%= content_for(:title, "Work Orders") %>
<h1>Listing User Accounts</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |current_user| %>
      <tr>
        <td><%= current_user.first_name %></td>
        <td><%= current_user.last_name %></td>
        <td><%= current_user.email %></td>
        <td><%= current_user.role.name%></td>
        <td><%= link_to 'Show', current_user %></td>
        <td><%= link_to 'Edit', edit_user_path(current_user) %></td>
        <td><%= link_to 'Destroy', current_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


  <%= link_to 'New User Account', new_user_path %>
